{
  "name": "ong-server",
  "version": "0.0.0",
  "engines": {
    "node": "14.x"
  },
  "private": true,
  "scripts": {
    "migrate": "npx sequelize db:migrate",
     "seed": "npx sequelize db:seed:all",
    "start": "node ./bin/www",
    "start:dev": "nodemon babel-node ./bin/www",
    "lint": "eslint . --ext .js",
    "lint:fix": "npm run lint:js -- --fix",
    "prettier": "prettier \"**/*.js\" --write --ignore-path .gitignore",
    "test": "mocha ./test/*.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.100.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.0.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.1",
    "express": "~4.16.1",
    "express-validator": "^6.14.1",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.2.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "6.2.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint": "7.32",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.0",
    "jest": "27.0.6",
    "lint-staged": "11.1.1",
    "mocha": "^9.1.3",
    "nodemon": "^2.0.16",
    "prettier": "2.3.2",
    "supertest": "^6.1.6"
  }
}
